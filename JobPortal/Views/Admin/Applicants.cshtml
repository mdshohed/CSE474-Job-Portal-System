@model IEnumerable<JobPortal2.Models.AppliedJob>

@{
    ViewBag.Title = "Applicants";
    Layout = "~/Views/Shared/_LayoutForAdmin.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">List of Applicant's   </h1>
    </div>
    <!-- /.col-lg-12 -->
</div>


<form class="wow fadeInDownaction" action="controller.php?action=delete" Method="POST">
    <table id="dash-table" class="table table-striped  table-hover table-responsive" style="font-size:12px" cellspacing="0">

        <thead>
            <tr>
                <th>@Html.DisplayNameFor(model => model.AppliedId)</th>
                <th>@Html.DisplayNameFor(model => model.CompanyName)</th>
                <th>@Html.DisplayNameFor(model => model.JobTitle)</th>
                <th>@Html.DisplayNameFor(model => model.UserName)</th>
                <th>@Html.DisplayNameFor(model => model.UserEmail)</th>
                <th>@Html.DisplayNameFor(model => model.ContactNo)</th>
            </tr>
        </thead>
        <tbody>
           
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AppliedId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CompanyName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.JobTitle)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserEmail)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ContactNo)
                </td>
            </tr>
            }

        </tbody>

        </table>


</form>